<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.locaplace.host.product.mapper.ProductMapper">
    <select id="selectTestList" resultType="com.locaplace.test.dto.TestDto">
    	select   
    		
    	from product
 	</select>
 	<insert id="insertProduct">
 		insert into product(
 		str
 		)values(#{str})
 	</insert>
 	<update id="updateProduct">
 		update product set
 		num = #{num}
 		where str=#{str}
 	</update>
</mapper>